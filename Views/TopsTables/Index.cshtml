@model IEnumerable<GoVintage.Models.TopsTable>

@{
    ViewBag.Title = "Index";
}

<h2>Tops</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>



<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-sm-6 col-md-4">
            <div class="thumbnail">
                <div class="caption">
                    <h3>@Html.DisplayFor(modelItem => item.TopsName)</h3>
                    <img src="@Url.Content(item.TopsPhoto)" />  
                </div>
                @Html.ActionLink("Edit", "Edit", new { id = item.TopsID }) |
                @Html.ActionLink("Details", "Details", new { id = item.TopsID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.TopsID })
            </div>
        </div>
    }
</div>

